<script>
$('.faq-list dt').click(function() {

    var myList = $(this).next('dd');

    if(!$(this).hasClass('selected')) {
        $('.faq-list dt').removeClass('selected');
        $(this).addClass('selected');
        $('.faq-list dd').hide();
        myList.show();
    } else {

        $(this).removeClass('selected');
        myList.hide();
    }

    return false;
});
</script>